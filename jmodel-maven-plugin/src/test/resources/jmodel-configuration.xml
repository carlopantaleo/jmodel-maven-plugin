<?xml version="1.0" encoding="UTF-8"?>
<jmodel-configuration
        xmlns="http://jmodel.carlopantaleo.com/JModel-configuration/1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://jmodel.carlopantaleo.com/JModel-configuration/1.0.0 /jmodel-configuration.xsd">

    <generators>
        <java-generator>
            <destination-package>com.jmodel.generated</destination-package>
        </java-generator>
        <hbm-generator>
            <destination-dao-package>com.jmodel.dao.generated</destination-dao-package>
            <destination-hbm-dir>generated</destination-hbm-dir>
        </hbm-generator>
        <typescript-generator>
            <destination-dir>src/main/typescript/generated</destination-dir>
        </typescript-generator>
    </generators>

</jmodel-configuration>